{
  "name": "neo-keybinds",
  "displayName": "neo-keybinds",
  "description": "Alternative keybindings for vscode with cheatsheet",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Keymaps"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "neo-keybinds.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "neo-keybinds.backwards",
        "title": "neo-keybinds (â†¹)"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+'",
        "command": "neo-keybinds.helloWorld"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "neo-keybinds.backwards",
          "group": "navigation",
          "when": "resourceScheme == neo-keybinds"
        }
      ],
      "commandPalette": [
        {
          "command": "neo-keybinds.backwards",
          "when": "resourceScheme == neo-keybinds"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}